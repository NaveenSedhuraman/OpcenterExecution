<FieldsDirectory>

  <!-- Map by field attribute -->
  <FieldMapEntry IsList="True">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebGridControls.JQDataGrid"/>
  </FieldMapEntry>

  <FieldMapEntry IsList="True" IsEnum="True">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebGridControls.JQDataGrid"/>
  </FieldMapEntry>

  <FieldMapEntry IsEnum="True" IsReadOnly="True">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.ReadOnlyEnum"/>
  </FieldMapEntry>
  
  <FieldMapEntry IsEnum="True">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.DropDownList"/>
  </FieldMapEntry>
  <!--  -->

  <!-- Map by field names -->
  <FieldMapEntry FieldNames="DiscoveryArea">
    <ControlLibrary ControlType="Camstar.WebPortal.WebPortlets.DiscoveryAreaControl"/>
  </FieldMapEntry>
  <!--  -->

  <!-- Map by field types -->
  <FieldMapEntry TypeNames="String,Int32,Double,EncryptedField">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.TextBox">
      <DefaultValue Name="ReadOnly" Value="False"/>
      <DefaultValue Name="Required" Value="False"/>
    </ControlLibrary>
  </FieldMapEntry>

  <FieldMapEntry TypeNames="Boolean">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.CheckBox">
      <DefaultValue Name="ReadOnly" Value="False"/>
    </ControlLibrary>
  </FieldMapEntry>

  <FieldMapEntry TypeNames="NamedObjectRef">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.NamedObject">
      <DefaultValue Name="ReadOnly" Value="False"/>
    </ControlLibrary>
  </FieldMapEntry>

  <FieldMapEntry TypeNames="SubentityRef,NamedSubentityRef">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.NamedSubentity">
      <DefaultValue Name="ReadOnly" Value="False"/>
    </ControlLibrary>
  </FieldMapEntry>

  <FieldMapEntry TypeNames="RevisionedObjectRef">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.RevisionedObject">
      <DefaultValue Name="ReadOnly" Value="False"/>
    </ControlLibrary>
  </FieldMapEntry>

  <FieldMapEntry TypeNames="ContainerRef">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebGridControls.ContainerListGrid" />
  </FieldMapEntry>

  <FieldMapEntry TypeNames="DateTime">
    <ControlLibrary ControlType="Camstar.WebPortal.FormsFramework.WebControls.DateChooser"/>
  </FieldMapEntry>
  <!--  -->

</FieldsDirectory>
